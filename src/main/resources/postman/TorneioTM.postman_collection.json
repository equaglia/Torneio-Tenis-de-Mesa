				{
					"name": "Soma 1 ponto",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"// for generating random number \r",
									"function randomNumber(min, max) { \r",
									"    return Math.random() * (max - min) + min;\r",
									"} \r",
									"\r",
									"function randomInteger(min, max) {\r",
									"   return Math.round((Math.random() * Math.abs(max - min)) + min);\r",
									"}\r",
									"\r",
									"function somas(valor, vezes){\r",
									"    let resultado = valor;\r",
									"    let contar = 0;\r",
									"    while (contar < vezes) {\r",
									"        resultado++;\r",
									"        contar++;\r",
									"    }\r",
									"    return resultado;\r",
									"}\r",
									"\r",
									"setTimeout(function () { }, [pm.environment.get(\"_tempoCurto\")]);\r",
									"var partidaStatus = pm.environment.get(\"_partidaStatus\");\r",
									"var gameReferencia = pm.environment.get(\"_gameReferencia\");\r",
									"var PontoIdReferencia = pm.environment.get(\"_pontoIdReferencia\");\r",
									"var PontoIdCandidatoA = pm.environment.get(\"_pontoIdCandidatoA\");\r",
									"var PontoIdCandidatoB = pm.environment.get(\"_pontoIdCandidatoB\");\r",
									"var partida = pm.environment.get(\"_partida\");\r",
									"if (partidaStatus == \"Finalizado\" || partidaStatus == \"Cancelado\"){\r",
									"    partida = somas(partida, 1);\r",
									"    pm.environment.set(\"_partida\", partida);\r",
									"    \r",
									"    gameReferencia = pm.environment.get(\"_gameInicial\");\r",
									"    gameReferencia = somas(gameReferencia, 5);\r",
									"    pm.environment.set(\"_game\", gameReferencia);\r",
									"    pm.environment.set(\"_gameInicial\", gameReferencia);\r",
									"    pm.environment.set(\"_gameReferencia\", gameReferencia);\r",
									"\r",
									"    PontoIdReferencia = pm.environment.get(\"_pontoIdInicial\");\r",
									"    PontoIdReferencia = somas(PontoIdReferencia, 10);\r",
									"    pm.environment.set(\"_pontoId\", PontoIdReferencia);\r",
									"    pm.environment.set(\"_pontoIdInicial\", PontoIdReferencia);\r",
									"    pm.environment.set(\"_pontoIdReferencia\", PontoIdReferencia);\r",
									"\r",
									"    PontoIdCandidatoA = PontoIdReferencia;\r",
									"    PontoIdCandidatoB = somas(PontoIdReferencia, 1);\r",
									"    pm.environment.set(\"_pontoIdCandidatoA\", PontoIdCandidatoA);\r",
									"    pm.environment.set(\"_pontoIdCandidatoB\", PontoIdCandidatoB);\r",
									"    postman.setNextRequest(\"Continuar Partida\");\r",
									"} else {\r",
									"    pm.test(\"Status code is 200\", function() {\r",
									"        var jsonData = JSON.parse(responseBody);\r",
									"        var currentGame = pm.environment.get(\"_game\");\r",
									"        var gameStatus = jsonData.gameStatus;\r",
									"        console.log(\"partida \"+partida+\" = \"+partidaStatus+\"     game \"+currentGame+\" = \"+gameStatus+\"    PontoId = \"+pm.environment.get(\"_pontoId\"));\r",
									"        if (partidaStatus == \"EmAndamento\" || partidaStatus == \"Preparado\") {\r",
									"            if (gameStatus == \"Finalizado\") {\r",
									"                let nextGame = somas(currentGame, 1);\r",
									"                pm.environment.set(\"_game\", nextGame);\r",
									"                PontoIdReferencia = somas(PontoIdReferencia, 2);\r",
									"                PontoIdCandidatoA = PontoIdReferencia;\r",
									"                PontoIdCandidatoB = somas(PontoIdReferencia, 1);\r",
									"                pm.environment.set(\"_pontoId\", PontoIdReferencia);\r",
									"                pm.environment.set(\"_pontoIdReferencia\", PontoIdReferencia);\r",
									"                pm.environment.set(\"_pontoIdCandidatoA\", PontoIdCandidatoA);\r",
									"                pm.environment.set(\"_pontoIdCandidatoB\", PontoIdCandidatoB);\r",
									"                postman.setNextRequest(\"Continuar Partida\");\r",
									"            } else {\r",
									"                pm.environment.set(\"_pontoId\", randomInteger(PontoIdCandidatoA, PontoIdCandidatoB));\r",
									"                postman.setNextRequest(\"Soma 1 ponto\");\r",
									"            }\r",
									"        } else if (partidaStatus == \"Interrompido\"){\r",
									"            postman.setNextRequest(\"Continuar Partida\");\r",
									"        }\r",
									"        pm.response.to.have.status(200);\r",
									"    });\r",
									"}"
								],
								"type": "text/javascript"
							}
						}
